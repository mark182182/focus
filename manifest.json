{
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Focus settings",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "description": "Block tabs that are counter productive",
  "manifest_version": 3,
  "name": "Focus",
  "permissions": ["tabs", "storage", "scripting"],
  "version": "1.0"
}
